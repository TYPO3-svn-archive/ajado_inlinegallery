<T3DataStructure>
  <meta>
   <langDisable>1</langDisable>
  </meta>
  <sheets>
   <sDEF>
    <ROOT>
     <TCEforms>
      <sheetTitle>Gallery</sheetTitle>
     </TCEforms>
     <type>array</type>
      <el>
      	<image>
			<TCEforms>
	      		<label>Images</label>
	      		<config>
	      			<type>group</type>
	      			<internal_type>file</internal_type>
	      			<allowed>jpg,jpeg,gif,png</allowed>
	      			<max_size>10000</max_size>
	      			<uploadfolder>uploads/tx_ajadoinlinegallery</uploadfolder>
	      			<size>10</size>
	      			<minitems>0</minitems>
	      			<maxitems>1000</maxitems>
	      		</config>
	      	</TCEforms>
      	</image>
      	
      	<alttext>
      		<TCEforms>
			    <label>Alt Text for images</label>
				<config>
					<type>text</type>
					<rows>10</rows>
					<cols>48</cols>
				</config>
			</TCEforms>
      	</alttext>
      	
      	<widthheight>
      		<TCEforms>
			    <label>Preview Image Width x Height, zB. &quot;400x200&quot;</label>
				<config>
					<type>text</type>
					<rows>10</rows>
					<cols>48</cols>
				</config>
			</TCEforms>
      	</widthheight>
      </el>
     </ROOT>
    </sDEF>
   </sheets>
 </T3DataStructure>